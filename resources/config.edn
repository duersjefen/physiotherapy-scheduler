{:server {:port #profile {:dev 3000
                         :test 3001
                         :prod #env PORT}}
 
 :database {:db-file #profile {:dev "physiotherapy-dev.db"
                              :test "physiotherapy-test.db"
                              :prod #env DATABASE_FILE}}
 
 :auth {:secret #profile {:dev "dev-secret-key-change-in-production"
                         :prod #env AUTH_SECRET}
        :session-timeout-minutes 60}
 
 :logging {:level #profile {:dev :debug
                           :test :warn
                           :prod :info}}}
